{
  "name": "ipod",
  "version": "0.2.1",
  "description": "command line interface mp3 player based on Node.js",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "config": {
    "blanket": {
      "pattern": "ipod/lib",
      "data-cover-flags": {
        "debug": false
      }
    },
    "travis-cov": {
      "threshold": 98
    }
  },
  "bin": {
    "ipod": "bin/ipod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JacksonTian/ipod.git"
  },
  "keywords": [
    "player",
    "cli",
    "mp3",
    "ipod"
  ],
  "author": "Jackson Tian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JacksonTian/ipod/issues"
  },
  "dependencies": {
    "speaker": "*",
    "lame": "*",
    "optimist": "*",
    "debug": "*"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "blanket": "*",
    "travis-cov": "*",
    "mocha-lcov-reporter": "*"
  }
}
